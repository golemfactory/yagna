    }
  ],
  "compManifest": {
    "version": "0.1.0",
    "script": {
      "commands": [
        "run .*curl.*",
        "run .*request.sh.*",
        "transfer .*output.txt"
      ],
      "match": "regex"
    },
    "net": {
